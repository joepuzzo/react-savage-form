{
  "name": "react-savage-form",
  "version": "1.0.29",
  "description": "A non shitty react form lib",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha-webpack --opts test/mocha-webpack.opts",
    "test:watch": "npm run test -- --watch",
    "start": "NODE_ENV=development webpack-dev-server -d --config config/webpack/webpack.dev.config.js --port 4005 --hot",
    "build:dev": "NODE_ENV=development webpack --config config/webpack/webpack.dev.config.js",
    "build:dist": "NODE_ENV=production webpack -p --config config/webpack/webpack.dist.config.js",
    "build:prod": "NODE_ENV=production webpack -p --config config/webpack/webpack.prod.config.js",
    "eslint": "eslint src/** --fix"
  },
  "author": {
    "name": "Joseph Puzzo",
    "email": "jgpuzzo2@gmail.com",
    "url": "http://joepuzzo.github.io"
  },
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "react-redux",
    "redux-form",
    "form",
    "react-form"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/joepuzzo/react-savage-form.git"
  },
  "license": "ISC",
  "dependencies": {
    "prop-types": "^15.5.10",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bootstrap": "^4.0.0-alpha.6",
    "chai": "^4.1.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.5",
    "enzyme": "^2.9.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "jsdom": "^9.9.1",
    "mocha": "^3.5.0",
    "mocha-webpack": "^0.7.0",
    "node-sass": "^4.5.3",
    "prismjs": "^1.6.0",
    "raw-loader": "^0.5.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-prism": "^4.3.0",
    "sass-loader": "^6.0.6",
    "sinon": "^3.2.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "webpack-node-externals": "^1.6.0"
  },
  "babel": {
    "presets": [
      "env",
      "stage-0",
      "react"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  }
}
